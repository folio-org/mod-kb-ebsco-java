          "methods": ["POST", "DELETE"],
          "pathPattern": "/_/tenant"
        }
      ]
    },
    {
      "id": "_timer",
      "version": "1.0",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": [ "POST" ],
          "pathPattern": "/loadHoldings",
          "modulePermissions": ["configuration.entries.collection.get"],
          "unit": "day",
          "delay": "5"
        }
      ]
