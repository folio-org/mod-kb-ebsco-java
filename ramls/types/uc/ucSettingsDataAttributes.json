{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Usage Consolidation Settings Data Attributes Schema",
  "description": "Usage Consolidation Settings Data Attributes Schema",
  "javaType": "org.folio.rest.jaxrs.model.UCSettingsDataAttributes",
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "credentialsId": {
      "type": "string",
      "description": "The UUID of credentials",
      "$ref": "../../raml-util/schemas/uuid.schema",
      "examples": "2ffa1940-2cf6-48b1-8cc9-5e539c61d93f"
    },
    "customerKey": {
      "type": "string",
      "description": "Encrypted customer ID",
      "examples": "NS12345TS"
    },
    "startMonth": {
      "type": "string",
      "description": "Fiscal starting month",
      "$ref": "month.json"
    },
    "currency": {
      "type": "string",
      "description": "Currency",
      "examples": "USD"
    },
    "platformType": {
      "type": "string",
      "description": "Type of platform",
      "$ref": "platformType.json"
    }
  },
  "required": [
    "customerKey",
    "currency"
  ]
}
