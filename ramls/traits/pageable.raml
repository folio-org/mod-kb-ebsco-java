queryParameters:
  page:
    displayName: Page offset
    type: integer
    minimum: 1
    maximum: 2147483647
    description: Page offset to retrieve results from Ebsco KB
    example: 1
    default: 1
    required: false
  count:
    displayName: Count
    type: integer
    minimum: 0
    maximum: 100
    description: Count of number of results to retrieve from Ebsco KB
    example: 100
    default: 25
    required: false
