queryParameters:
  page:
    displayName: Page offset
    type: integer
    minimum: 1
    maximum: 2147483647
    description: Page number
    example: 1
    default: 1
    required: false
  count:
    displayName: Count
    type: integer
    minimum: 1
    maximum: <<maxCountValue>>
    description: Page size
    example: 100
    default: <<defaultCountValue>>
    required: false
